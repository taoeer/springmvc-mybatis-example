<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.taoeer.simplecms.mapper.UserMapper">
    <insert id="save" parameterType="top.taoeer.simplecms.domain.User" useGeneratedKeys="true">
        INSERT INTO users(name, sex, age)
        VALUES (#{name}, #{sex}, #{age})
    </insert>
</mapper>